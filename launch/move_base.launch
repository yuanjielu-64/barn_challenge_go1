<launch>
    <arg name="frame_prefix" default=""/>
    <arg name="rname" default=""/>

    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <rosparam file="$(find barn_challenge_lu)/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find barn_challenge_lu)/params/costmap_common_params.yaml" command="load" ns="local_costmap" />

        <rosparam file="$(find barn_challenge_lu)/params/odom_nav_params/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find barn_challenge_lu)/params/odom_nav_params/local_costmap_params.yaml" command="load" />

        <rosparam file="$(find barn_challenge_lu)/params/base_local_planner_params.yaml" command="load" />
        <rosparam file="$(find barn_challenge_lu)/params/move_base_params.yaml" command="load" />
            
        <rosparam file="$(find barn_challenge_lu)/params//global_planner_params.yaml" command="load" />

        <!-- explicitly define frame ids for movebase -->
        <!-- <param name="global_costmap/global_frame"                   value="map"/>
        <param name="global_costmap/robot_base_frame"               value="base"/>
        <param name="global_costmap/2d_obstacles_layer/scan/topic"  value="/go1_gazebo/scan"/>
        <param name="global_costmap/3d_obstacles_layer/depth/topic" value="/cam1/point_cloud_face"/>
        <param name="local_costmap/global_frame"                    value="odom"/>
        <param name="local_costmap/robot_base_frame"                value="base"/>
        <param name="local_costmap/2d_obstacles_layer/scan/topic"   value="/go1_gazebo/scan"/>
        <param name="local_costmap/3d_obstacles_layer/depth/topic"  value="/cam1/point_cloud_face"/> -->

        <param name="base_global_planner" type="string" value="navfn/NavfnROS" />
        <param name="base_local_planner" value="base_local_planner/TrajectoryPlannerROS"/>

        <remap from="odom" to="odometry/filtered" />
        <!--<remap from="cmd_vel" to="cmd_vel_mb"/> -->

    </node>
</launch>